{
    "version": 2,
    "builds": [
      {
        "src": "project_conflict_finder/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "220mb",
          "runtime": "Python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "project_conflict_finder/wsgi.py"
      }
    ]
  }
  